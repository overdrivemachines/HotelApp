<p id="notice"><%= notice %></p>

<h1>Listing Room Type Rates</h1>

<table>
  <thead>
    <tr>
      <th>Room type</th>
      <th>On date</th>
      <th>Rate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @room_type_rates.each do |room_type_rate| %>
      <tr>
        <td><%= room_type_rate.room_type %></td>
        <td><%= room_type_rate.on_date %></td>
        <td><%= room_type_rate.rate %></td>
        <td><%= link_to 'Show', room_type_rate %></td>
        <td><%= link_to 'Edit', edit_room_type_rate_path(room_type_rate) %></td>
        <td><%= link_to 'Destroy', room_type_rate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Room type rate', new_room_type_rate_path %>
