<p id="notice"><%= notice %></p>

<h1>Listing Room Types</h1>

<table>
  <thead>
    <tr>
      <th>Property</th>
      <th>Code</th>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @room_types.each do |room_type| %>
      <tr>
        <td><%= room_type.property %></td>
        <td><%= room_type.code %></td>
        <td><%= room_type.name %></td>
        <td><%= room_type.description %></td>
        <td><%= link_to 'Show', room_type %></td>
        <td><%= link_to 'Edit', edit_room_type_path(room_type) %></td>
        <td><%= link_to 'Destroy', room_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Room type', new_room_type_path %>
